  <set xpath="//item[@group='weaponsGunsT1']/@group">weaponsGunsCommon</set>
  <set xpath="//item[@group='weaponsGunsT2']/@group">weaponsGunsAdvanced</set>
  <set xpath="//item[contains(@group, 'weaponsGunsT3')]/@group">weaponsGunsMilitary</set>
  <set xpath="//item[@group='groupArmorLightT2']/@group">groupArmorLight</set>
  <set xpath="//item[@group='groupArmorHeavyT2']/@group">groupArmorHeavy</set>
  <set xpath="//item[@group='vehiclePartsMixed']/@group">vehicleParts</set>
  <set xpath="//item[@group='perkBooks']/@group">books</set>
  <setattribute xpath="//trader_info[@id='75']/*/item[@group='hazmatGear']" name="prob">0.1</setattribute>
</configs>
